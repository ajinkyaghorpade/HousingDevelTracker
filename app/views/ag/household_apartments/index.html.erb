<p id="notice"><%= notice %></p>

<h1>Ag Household Apartments</h1>

<table>
  <thead>
    <tr>
      <th>Moved</th>
      <th>Moved out</th>
      <th>Cost</th>
      <th>Apartment</th>
      <th>Household</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ag_household_apartments.each do |ag_household_apartment| %>
      <tr>
        <td><%= ag_household_apartment.moved_id %></td>
        <td><%= ag_household_apartment.moved_out %></td>
        <td><%= ag_household_apartment.cost %></td>
        <td><%= ag_household_apartment.apartment %></td>
        <td><%= ag_household_apartment.household %></td>
        <td><%= link_to 'Show', ag_household_apartment %></td>
        <td><%= link_to 'Edit', edit_ag_household_apartment_path(ag_household_apartment) %></td>
        <td><%= link_to 'Destroy', ag_household_apartment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ag Household Apartment', new_ag_household_apartment_path %>
